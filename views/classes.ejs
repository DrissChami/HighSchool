<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8"/>
    <link rel="stylesheet" href="/css/style.css">

    <title>Liste des classes</title>
</head>

<body>
    <% classes.forEach(function(classe){ %>
        <a href="/classe/<%= classe.id_classe%>">
            <%= classe.niveau %>
            <%= classe.nom %>
            <%= classe.annee %>
        </a><br>
    <% }); %>
</body>
</html>